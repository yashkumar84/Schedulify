{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "nodemon --watch . --ext js,jsx,ts,tsx,html --exec \"npm run lint:continue\"",
    "lint:continue": "eslint . || exit 0",
    "start:dev": "concurrently \"npm run lint:watch\" \"NODE_ENV=dev nodemon server.js\"",
    "start": "node server.js",
    "staging": "NODE_ENV=staging forever start --inspect --max-old-space-size=8192 server.js",
    "stagingrestart": "NODE_ENV=staging forever restart --inspect --max-old-space-size=8192 server.js",
    "stagingstop": "NODE_ENV=staging forever stop --inspect --max-old-space-size=8192 server.js",
    "stagingtest": "NODE_ENV=staging node --inspect --max-old-space-size=8192 server.js",
    "prodtest": "NODE_ENV=prod node --inspect --max-old-space-size=8192 server.js",
    "prod": "NODE_ENV=prod forever start --inspect --max-old-space-size=8192 server.js",
    "prodrestart": "NODE_ENV=prod forever restart --inspect --max-old-space-size=8192 server.js",
    "prodstop": "NODE_ENV=prod forever stop --inspect --max-old-space-size=8192 server.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.2.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "@aws-sdk/client-s3": "^3.714.0",
    "nodemailer": "^8.0.1",
    "socket.io": "^4.8.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "nodemon": "^3.1.11"
  }
}